<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>


<AnchorPane prefHeight="96.0" prefWidth="154.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="es.ieslosmontecillos.appinformes.AppInformes">
   <children>
      <VBox prefHeight="107.0" prefWidth="154.0">
         <children>
            <Label text="Introduce el Id del cliente">
               <VBox.margin>
                  <Insets left="10.0" top="10.0" />
               </VBox.margin>
            </Label>
            <TextField fx:id="txtIdCliente" maxWidth="-Infinity" prefHeight="25.0" prefWidth="130.0">
               <VBox.margin>
                  <Insets left="10.0" top="5.0" />
               </VBox.margin>
            </TextField>
            <Button fx:id="btnGenerar" mnemonicParsing="false" prefHeight="25.0" prefWidth="129.0" text="Generar informe" onAction="#generaFacturas_por_Cliente">
               <VBox.margin>
                  <Insets left="10.0" top="10.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
